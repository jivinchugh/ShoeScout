{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "plugin:jest/recommended",
    "prettier"

  ],
  "plugins": ["react", "react-hooks", "prettier", "jest"],
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "rules": {
    "prettier/prettier": ["error"]
  },

  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
